{
  "name": "tcg-comparison-widget",
  "version": "0.1.5",
  "authors": [
    "Darren Labey <darren.labey@thecontrolgroup.com>"
  ],
  "description": "Re-usable Polymer 0.5.* comparison widget that can be used to display a main metric and compare it against up to 3 other metrics.",
  "keywords": [
    "web-component",
    "web-components",
    "polymer",
    "tcg",
    "comparison",
    "widget"
  ],
  "main": "tcg-comparison-widget.html",
  "license": "MIT",
  "homepage": "https://github.com/the-control-group/tcg-comparison-widget/",
  "ignore": [
    "/.*",
    "/tests/"
  ],
  "dependencies": {
    "polymer": "Polymer/polymer#~0.5",
    "polymer-core-icons": "Polymer/core-icons#~0.5",
    "highcharts": "highslide-software/highcharts-release#~v4.1",
    "lodash": "lodash/lodash#~v3.8",
    "numeral-js": "adamwdraper/Numeral-js#~1.5"
  },
  "devDependencies": {
    "web-component-tester": "*"
